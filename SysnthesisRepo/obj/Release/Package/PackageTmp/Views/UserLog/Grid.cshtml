        display: flex;
        -webkit-box-orient: vertical;
        -webkit-box-direction: normal;
        -ms-flex-direction: column;
        flex-direction: column;
        -webkit-box-pack: start;
        -ms-flex-pack: start;
        justify-content: flex-start;
    }

        .itenInfoContainer a {
            text-decoration: none;
            font-size: 12px;
            margin-top: 5px;
            color: #596dff;
        }

        .itenInfoContainer ul.gridProductInfo {
            list-style: none;
            padding: 0px;
            margin: 0px;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-orient: horizontal;
            -webkit-box-direction: normal;
            -ms-flex-direction: row;
            flex-direction: row;
            -ms-flex-wrap: wrap;
            flex-wrap: wrap;
            -webkit-box-pack: start;
            -ms-flex-pack: start;
            justify-content: flex-start;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
        }

            .itenInfoContainer ul.gridProductInfo li:nth-child(1) {
                font-size: 12px;
                background: #dfdfdf;
                border: 1px solid #b7b7b7;
                padding: 2px 4px;
                border-radius: 5px;
                margin-right: 5px;
            }

            .itenInfoContainer ul.gridProductInfo li:nth-child(2) {
                font-size: 12px;
                padding: 3px 5px;
                border-radius: 5px;
                margin-right: 5px;
            }

    #gridvaluechange_toolbarItems.e-sticky {
        top: 145px !important;
    }

    .tooltiosection .tooltipTop:hover .top {
        display: block;
        left: 50%;
        top: 0;
    }

    .tooltiosection .tooltipTop {
        border: none !important;
    }

    .qtyTooltipContainer {
        display: flex;
        font-family: Maven Pro;
        align-items: flex-start;
        justify-content: flex-start;
        background: transparent;
        width: auto;
        box-sizing: border-box;
        border-radius: 5px;
    }

        .qtyTooltipContainer ul {
            display: flex;
            padding: 0px;
            list-style: none;
            width: 100%;
            flex-direction: column;
            gap: 10px;
            user-select: none;
        }

            .qtyTooltipContainer ul li {
                display: flex;
                gap: 10px;
                width: 100%;
                justify-content: space-between;
                align-items: center;
            }

                .qtyTooltipContainer ul li span:nth-child(1) {
                    display: block;
                    background: Transparent;
                    color: white;
                    height: 10px;
                    padding: 3px 5px;
                    border-radius: 3px;
                    font-size: 12px;
                    line-height: 10px;
                }

                .qtyTooltipContainer ul li span:nth-child(2) {
                    display: block;
                    background: Transparent;
                    color: white;
                    height: 14px;
                    padding: 3px 5px;
                    border-radius: 3px;
                    font-size: 14px;
                    line-height: 14px;
                }
</style>
