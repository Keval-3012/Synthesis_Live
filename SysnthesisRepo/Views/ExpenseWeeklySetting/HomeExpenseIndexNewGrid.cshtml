<div id="loadbox"></div>
<div id="grddata">
    <div class="animate-panelmessesgarea padbtmzero">
    </div>
    @Html.EJS().Toast("toast_type").NewestOnTop(true).ShowCloseButton(true).Position(p => p.X("Right")).TimeOut(5000).Render()


    <div class="addinvoice" style="padding-top:0px!important;">
        <input type="hidden" id="hdnpagevalue" value="0" />
        <div class="page-body headertopbox special_bg">
            <div class="page-header-main position-relative" style="height:80px !important">
                <h2 style="margin-top:18px; margin-left:20px">Homesome Expenses</h2>
                <div style="display:flex;">

                    <div style="max-width:180px; margin-top:10px">
                        <label>StartDate</label>
                        @Html.EJS().DatePicker("StartDate").Format("MM-dd-yyyy").Change("OnChangeStartDate").Max(DateTime.Now).Render()
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div style="max-width: 180px; margin-top: 10px">
                        <label>EndDate</label>
                        @Html.EJS().DatePicker("EndDate").Format("MM-dd-yyyy").Change("OnChangeEndDate").Max(DateTime.Now).Render()
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <div style="display:flex;align-items:center;justify-content:center;padding-top:30px;">
                        <a href="javascript:viewdata();" class="viewBtnUnclearCheck" data-toggle="tooltip" data-placement="top" data-original-title="" title="">Filter</a>
                        &nbsp;&nbsp;&nbsp;&nbsp;
                        <button type="button" class="resetBtnUnclearCheck" onclick="reset();">Reset</button>
                    </div>
                </div>
            </div>


            @if (storeid == "0")
            {


                <div class="control-section">

                    @Html.EJS().Grid("InlineExpenseEditing").AllowSorting().AllowFiltering().EnableInfiniteScrolling().Height("90vh").FilterSettings(filter => { filter.Type(Syncfusion.EJ2.Grids.FilterType.CheckBox); }).DataSource(dm => dm.Url("/ExpenseWeeklySetting/UrlDatasourceExpense").Adaptor("UrlAdaptor")).SelectionSettings(sel => { sel.PersistSelection(true); }).EnableStickyHeader(true).AllowTextWrap().Columns(col =>
               {


                   col.Field("NickName").HeaderText("Store Name").Width("50").Add();
                   col.Field("PaymentFees").HeaderText("Payment Fees").Width("50").Format("$ ###0.#0").Add();
                   col.Field("DeliveryCost").HeaderText("Delivery Cost").Width("50").Format("$ ###0.#0").Add();
                   col.Field("TipsPaid").HeaderText("Tips Paid").Width("50").Format("$ ###0.#0").Add();
                   col.Field("ExpenseDate").HeaderText("Expense Date").Format("MM-dd-yyyy").Width("60").Add();
                   col.Field("Status").HeaderText("Status").Width("50").Template("#Status").AllowSorting(false).Add();
                   col.Field("PaymentFeesStatus").HeaderText("QB Status").Width("50").Template("#PaymentFeesStatus").AllowSorting(false).Add();
               }).DataBound("dataBound").ActionBegin("ActionBegin").PageSettings(page => page.PageSize(50)).Toolbar(new List<string>() { "Search", "ExcelExport", "PdfExport" }).ToolbarClick("toolbarClick").Render()


                </div>
            }
            else
            {
                <div class="control-section">

                    @Html.EJS().Grid("InlineExpenseEditing").AllowSorting().AllowFiltering().EnableInfiniteScrolling().Height("90vh").FilterSettings(filter => { filter.Type(Syncfusion.EJ2.Grids.FilterType.CheckBox); }).DataSource(dm => dm.Url("/ExpenseWeeklySetting/UrlDatasourceExpense").Adaptor("UrlAdaptor")).SelectionSettings(sel => { sel.PersistSelection(true); }).EnableStickyHeader(true).AllowTextWrap().Columns(col =>
               {

                   col.Field("PaymentFees").HeaderText("Payment Fees").Width("50").Format("$ ###0.#0").Add();
                   col.Field("DeliveryCost").HeaderText("Delivery Cost").Width("50").Format("$ ###0.#0").Add();
                   col.Field("TipsPaid").HeaderText("Tips Paid").Width("50").Format("$ ###0.#0").Add();
                   col.Field("ExpenseDate").HeaderText("Expense Date").Format("MM-dd-yyyy").Width("60").Add();
                   col.Field("Status").HeaderText("Status").Width("50").Template("#Status").AllowSorting(false).Add();
                   col.Field("PaymentFeesStatus").HeaderText("QB Status").Width("50").Template("#PaymentFeesStatus").AllowSorting(false).Add();

               }).DataBound("dataBound").ActionBegin("ActionBegin").PageSettings(page => page.PageSize(50)).Toolbar(new List<string>() { "Search", "ExcelExport", "PdfExport" }).ToolbarClick("toolbarClick").Render()


                </div>
            }


        </div>
    </div>
</div>

<script type="text/x-jsrender" id="PaymentFeesStatus">
    ${if(PaymentFeesStatus == "UnSynced")}
    <img src="/QBImages/NoQBSync.png" class="statusBadge" />
    ${else if(PaymentFeesStatus == "Pending")}
    <img src="/QBImages/pendingqbsync.png" class="statusBadge" />
    ${else}
    <img src="/QBImages/syncedqbsync.png" class="statusBadge">
    ${/if}
</script>
<script type="text/x-jsrender" id="DeliveryCostStatus">
    ${if(DeliveryCostStatus == "UnSynced")}
    <img src="/QBImages/NoQBSync.png" class="statusBadge" />
    ${else if(DeliveryCostStatus == "Pending")}
    <img src="/QBImages/pendingqbsync.png" class="statusBadge" />
    ${else}
    <img src="/QBImages/syncedqbsync.png" class="statusBadge">
    ${/if}
</script>
<script type="text/x-jsrender" id="TipsPaidStatus">
    ${if(TipsPaidStatus == "UnSynced")}
    <img src="/QBImages/NoQBSync.png" class="statusBadge" />
    ${else if(TipsPaidStatus == "Pending")}
    <img src="/QBImages/pendingqbsync.png" class="statusBadge" />
    ${else}
    <img src="/QBImages/syncedqbsync.png" class="statusBadge">
    ${/if}
</script>

<script type="text/x-jsrender" id="Status">
    ${if(Status == 0)}
    <a href="javascript:ChangeStatus(${ID});"><img src="/QBImages/Pending.png" class="statusBadge" /></a>
    ${else}
    <img src="/QBImages/Approved.png" class="statusBadge" />
    ${/if}
</script>
<script type="text/x-jsrender" id="amountTemplate">
    <span style="text-align:right">@Html.Raw("$ ")${Amount}</span>
</script>
