@model IEnumerable<EntityModels.Models.CustomersReceiveablesReceiptsListModel>
@using Syncfusion.EJ2

<link href="~/Content/ej2/bootstrap5.css" rel="stylesheet" />

<style>
    .page-header-main {
        padding: 0;
    }
</style>
<style>
    @@font-face {
        font-family: 'Toast_icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    #toast_types button {
        margin: 5px;
        min-width: 160px;
        max-width: 160px;
    }

    .toast-icons {
        font-family: 'Toast_icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    #toast_type .e-toast-icon.e-icons {
        height: auto;
        font-size: 30px;
    }

    .bootstrap5 #toast_type .e-toast-icon.e-icons,
    .bootstrap5-dark #toast_type .e-toast-icon.e-icons {
        height: 25px;
    }

    .toast-icons.e-success::before {
        content: "\e701";
    }

    .toast-icons.e-error::before {
        content: "\e700";
    }

    .toast-icons.e-info::before {
        content: "\e704";
    }

    .toast-icons.e-warning::before {
        content: "\e703";
    }

    .e-gridheader.e-lib.e-droppable.e-sticky {
        top: 191px !important;
    }

    .e-grid .e-toolbar {
        position: -webkit-sticky !important;
        position: sticky !important;
        top: 138px !important; /* The height of top nav menu. */
        z-index: 1 !important;
    }

    .hidedrp {
        display: none;
    }
</style>
<style>
    .control_wrapper {
        max-width: 400px;
        margin: 0 auto;
    }

    .e-upload {
        width: 100%;
        position: relative;
        margin-top: 15px;
    }

    .control_wrapper .e-upload .e-upload-drag-hover {
        margin: 0;
    }

    #reciptspopupid {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 9999;
        background: #010101b3;
    }

    #emailmodalpopup {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 9999;
        background: #010101b3;
    }

    #companynamemodalpopup {
        position: fixed;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 100%;
        z-index: 9999;
        background: #010101b3;
    }

    .radio-buttons-upload {
        max-width: 400px;
        margin: 10px auto;
        display: flex;
        gap: 10px;
    }

    .file-input-name {
        opacity: 0;
        display: none;
    }

    .toast.toast-error {
        background: #e53939;
        font-size: 14px;
    }
</style>
<style>
    #header-fixed {
        position: fixed;
        top: 0px;
        display: none;
        background-color: white;
    }

    .page-sidebar.sidebar-fixed {
        position: fixed;
    }

    body {
        color: #787878;
    }

    .page-header-main .searchinvoicebox input[type="text"] {
        width: 85% !important;
        border: 0px;
        height: unset !important;
        height: -1px !important;
    }

    .datagrid table tr td {
        color: #787878;
    }

    .datagrid a i {
        color: unset;
        font-size: 12px !important;
    }

    .fixed {
        top: 124px !important;
    }

    .btn-info,
    .btn-info:focus {
        background-color: #3C7BE5 !important;
        border-color: #3C7BE5;
        color: #ffffff;
    }
</style>
<style>
    .saa {
        background: #ff4948 !important;
        color: #fff !important;
        border: 1px solid #ff4948 !important;
        width: 100%;
        margin-bottom: 5px;
    }

    .modal-backdrop {
        z-index: 0 !important;
    }

    .e-icons.e-file-delete-btn {
        display: none !important;
    }

    .headertopbox .topRightBar {
        padding: 0px 0px !important;
        margin: 0 0 8px auto;
    }

    .upload-poppup {
        display: flex;
        justify-content: flex-end;
    }

    .button-green-email {
        background-color: lightgreen;
    }

    .mailapproved {
        color: white;
        background: green;
        padding: 0.2rem 0.5rem;
        border-radius: 0.2rem;
        font-weight: 600;
        border: 0.1rem solid rgba(2, 85, 2, 1);
        box-shadow: 0px 2px 6px rgba(0, 200, 0, 0.4);
        user-select: none;
    }

    .mailnotapproved {
        color: white;
        background: red;
        padding: 0.2rem 0.5rem;
        border-radius: 0.2rem;
        font-weight: 600;
        border: 0.1rem solid #ab0000;
        box-shadow: 0px 2px 6px rgba(233, 55, 55, 0.6);
        user-select: none;
    }
</style>
<div>
    @if (Convert.ToString(ViewBag.Storeidvalue) == "0")
    {
        <script>
            PopUpSelectStore()
        </script>
    }
</div>
<div id="popupStoreAlert" class="divIDClass modal-popup modal-danger modal-message" style="position: fixed;  left:45%; width:300px; top: 30%; display:none">
    <div class="modal-content ">
        <div class="modal-header text-center">
            <i class="glyphicon glyphicon-fire"></i>
        </div>
        <div class="modal-title">No Store Selected</div>
        <div class="modal-body ">Please Select Store.</div>
        <div class="modal-footer " style="text-align:center">
            <a class="btn btn-danger" data-dismiss="modal" onclick="closemodal()" ;>Ok </a>
        </div>
    </div>
</div>
@Html.EJS().Toast("toast_type").NewestOnTop(true).ShowCloseButton(true).Position(p => p.X("Right")).TimeOut(5000).Render()
<div class="header-small special_bg" style="margin: 0px;position:sticky;top:78px;z-index:99">
    <div class="row">
        <div class="col-md-6">
            <h3>Customers Receipts</h3>
        </div>
        @if (Convert.ToString(ViewBag.Storeidvalue) != "0")
        {
            <div class="col-md-6 upload-poppup">
                <a href="javascript:void(0)" class="btn btn-info btn-lg" onclick="OpenPopupUploadReciptsModel();"><i class="fa fa-cloud-upload" aria-hidden="true"></i>Upload Files</a>
            </div>
        }

    </div>

</div>


<div class="modal fade" id="reciptspopupid" role="dialog">
    <div class="modal-dialog modal-xl">
        <div class="modal-content" style="margin-top:14%;width:600px;">
            <div class="modal-header text-center">
                <button type="button" onclick="reloadpage()" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Upload Files</h4>
            </div>
            <div class="modal-body">
                <div class="control-section">
                    @*<div style="width:40%;">
                            @Html.EJS().DropDownList("CustomerReciptID").Fields(new Syncfusion.EJ2.DropDowns.DropDownListFieldSettings { Text = "CompanyName", Value = "CompanyNameId" }).Placeholder("Select Company Name").FloatLabelType(Syncfusion.EJ2.Inputs.FloatLabelType.Always).DataSource((IEnumerable<object>)ViewBag.CustomerReciptID).Render()
                        </div>*@
                    <div>
                        @Html.EJS().Uploader("UploadFiles").AllowedExtensions(".pdf,.jpeg,.png").SequentialUpload(true).Multiple(true).AsyncSettings(new Syncfusion.EJ2.Inputs.UploaderAsyncSettings { SaveUrl = @Url.Content("~/CustomerInformation/Save"), RemoveUrl = @Url.Content("~/CustomerInformation/Remove") }).Success("onImageUploadSuccess").AutoUpload(true).Render()
                    </div>
                </div>
                <input type="hidden" id="FileNameInvoice" />
            </div>
            <div class="modal-footer">
                <button type="button" onclick="reloadpage()" class="buttonbox btncancel" data-dismiss="modal">Cancel</button>
                <button type="button" onclick="SaveUploadedReciptFile()" class="buttonbox btnsubmit" data-dismiss="modal">Done</button>
            </div>
        </div>
    </div>
</div>


@if (Convert.ToString(ViewBag.Storeidvalue) != "0")
{
    <div class="datagrid martop15">
        <div class="table-responsive">

            <table class="table table-striped table-hover">

                <thead>
                    <tr>
                        <th width="45%"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">File Name</a></th>
                        <th width="10%" class="text-align-center"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">Company Name</a></th>
                        <th width="8%" class="text-align-center"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">Preview Email</a></th>
                        <th width="10%" class="text-align-center"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">Email Status</a></th>
                        <th width="12%" class="text-align-center"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">Email Sent time</a></th>
                        <th width="5%" class="text-align-center"><a class="textred" style="color:#fff; text-decoration:none; cursor:pointer;">Action</a></th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Count() > 0)
                    {
                        int index = 0;
                        foreach (var item in Model)
                        {
                            <tr class="even">
                                <td>
                                    @*<a href="/CustomerInformation/CustomerReceiptDownload/@item.CustomersReceiveablesReceiptsId" style="color: #3C7BE5">@item.FileName</a>*@
                                    <a href="/CustomerInformation/CustomerReceiptDownload/@item.CustomersReceiveablesReceiptsId" style="color: #3C7BE5" target="_blank">@item.FileName</a>
                                </td>
                                <td hidden>@item.CustomersReceiveablesReceiptsId</td>
                                @if (item.CompanyName == null)
                                {
                                    <td class="text-align-center">
                                        <button style="background-color: #3C7BE5; color: white; border: none; padding: 5px 12px; cursor: pointer; box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.7); " onclick="SelectCompanyNamePopup(@item.CustomersReceiveablesReceiptsId)">Assign</button>
                                    </td>
                                }
                                else
                                {
                                    <td class="text-align-center">
                                        <a href="#" style="color: #3C7BE5" onclick="SelectCompanyNamePopup(@item.CustomersReceiveablesReceiptsId)">@item.CompanyName</a>
                                    </td>
                                }
                                @if (item.CompanyName == null)
                                {
                                    <td class="text-align-center">
                                        <img src="~/Content/Admin/images/icons/mail-preview.svg" width="100" style="height:25px;opacity:0.3;" />
                                    </td>
                                }
                                else
                                {
                                    <td class="text-align-center">
                                        <img src="~/Content/Admin/images/icons/mail-preview.svg" width="100" style="height:25px;" onclick="OpenPopupSendEmailModel(@item.CustomersReceiveablesReceiptsId);" data-toggle="tooltip" data-original-title="Preview Email" />
                                    </td>
                                }
                                @if (item.IsEmailTriggered == false)
                                {
                                    if (item.IsEmailTriggered == false && item.CompanyName != null)
                                    {
                                        <td class="text-align-center">
                                            @*<img src="/Content/Admin/images/unmailed.png" alt="" style="width:80px; height:30px;" />*@
                                            <span class="mailnotapproved">Pending</span>
                                        </td>
                                    }
                                    else
                                    {
                                        <td class="text-align-center">
                                            <span>-</span>
                                        </td>
                                    }
                                }
                                else
                                {
                                    <td class="text-align-center">
                                        @*<img src="/Content/Admin/images/mailed.png" alt="" style="width:80px; height:30px;" />*@
                                        <span class="mailapproved">Sent</span>
                                    </td>
                                }
                                @if (item.IsEmailSentDate == null)
                                {
                                    <td class="text-align-center">
                                        <span>-</span>
                                    </td>
                                }
                                else
                                {
                                    <td class="text-align-center">@item.IsEmailSentDate</td>
                                }
                                <td>
                                    @{
                                        var CustomerApiKeyId = @item.CustomersReceiveablesReceiptsId + "D";
                                    }
                                    <a href="#" onclick="return ComfirmDelete(@item.CustomersReceiveablesReceiptsId);" data-toggle="tooltip" data-placement="top" data-original-title="Delete"><img src="../Content/Admin/images/trash-2.svg" alt="" width="50" /> </a>
                                    <div id="@CustomerApiKeyId" class="divIDClass modal-popup modal-danger modal-message" style="position: fixed;  left:45%; width:300px; top: 10px; display:none">
                                        <div class="modal-content ">
                                            <div class="modal-header text-center">
                                                <i class="glyphicon glyphicon-trash"></i>
                                            </div>
                                            <div class="modal-title">Message</div>
                                            <div class="modal-body ">Are you sure want to delete this User?</div>
                                            <div class="modal-footer" style="text-align:center">
                                                <a class="btn btn-danger" onclick="Delete(@item.CustomersReceiveablesReceiptsId);">Ok </a>
                                                <a class="btn" data-dismiss="modal" onclick="closemodal()">Cancel</a>
                                            </div>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            index++;
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="7" class="text-center">
                                No Data Available
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
}


<div class="modal fade" id="emailmodalpopup" role="dialog">
    <div class="modal-dialog modal-xl">
        <div class="modal-content" style="margin-top:14%;width:600px;">
            <div class="modal-header text-center">
                <button type="button" onclick="reloadpage()" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Email Preview</h4>
            </div>
            <div class="modal-body">

                <div class="control-section">
                    <div style="width:100%;padding:10px">
                        <p style="margin-bottom:30px;">Dear Customer,</p>
                        <p style="margin:10px 0px;">We hope this message finds you well. Attached to this email, you will find the detailed list of your recently ordered items along with the receipt.</p>
                        <p style="margin:10px 0px;">Please review the attached documents and process the payment at your earliest convenience. If you have any questions or require further assistance, please do not hesitate to reach out to us.</p>
                        <p style="margin:10px 0px;">Thank you for your prompt attention to this matter. We appreciate your business and look forward to serving you again.</p>
                        <p style="margin-top:30px;">Warm regards,<br></p>
                        <p style="font-weight:bold;">Westside Market NYC (110th street)</p>
                    </div>
                    <div style="width:100%;padding:10px">
                        <strong>Attached File:</strong> <span id="EmailFileName" style="color: #006CFF;font-weight:bold;cursor:pointer;" onclick="DownloadCustomerReceipt()"></span>
                    </div>
                    <input type="hidden" id="CustomersReceiveablesReceiptsId" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" onclick="reloadpage()" class="buttonbox btncancel" data-dismiss="modal">Cancel</button>
                <button type="button" id="sendEmailButton" onclick="SendEmailToCustomer()" class="buttonbox btnsubmit" data-dismiss="modal"></button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="companynamemodalpopup" role="dialog">
    <div class="modal-dialog modal-xl">
        <div class="modal-content" style="margin-top:15%;width:400px;">
            <div class="modal-header text-center">
                <button type="button" onclick="reloadpage()" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Select Company</h4>
            </div>
            <div class="modal-body">
                <div class="control-section">
                    <div style="width:60%;">
                        @Html.EJS().DropDownList("CustomerReciptID").Fields(new Syncfusion.EJ2.DropDowns.DropDownListFieldSettings { Text = "CompanyName", Value = "CompanyNameId" }).Placeholder("Select Company Name").FloatLabelType(Syncfusion.EJ2.Inputs.FloatLabelType.Always).DataSource((IEnumerable<object>)ViewBag.CustomerReciptID).Render()
                    </div>
                </div>
                <input type="hidden" id="CustomersReceiptsId" />
            </div>
            <div class="modal-footer">
                <button type="button" onclick="reloadpage()" class="buttonbox btncancel" data-dismiss="modal">Cancel</button>
                <button type="button" onclick="SaveCompanyName()" class="buttonbox btnsubmit" data-dismiss="modal">Done</button>
            </div>
        </div>
    </div>
</div>

<script id='dialogtemplates' type="text/x-template">
    <div id="dialogTemp">
    </div>
</script>

<script src="~/Form_JS/CustomerRecipts.js"></script>
<script>
    document.title = '@ViewBag.Title';
    function OpenPopupUploadReciptsModel() {
        $('#reciptspopupid').modal({
            backdrop: 'static',
            keyboard: true,
            show: true
        });
    }
    function reloadpage() {
        location.reload();
    }
    function closemodal() {
        $(".divIDClass").hide();
    }
</script>
@Html.EJS().ScriptManager()
