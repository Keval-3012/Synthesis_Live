@{
    Layout = "~/Views/Shared/AdminMAster.cshtml";
}

@{
    Html.RenderAction("CheckListIndexNewGrid");
}

<div id="popupStoreAlert" class="divIDClass modal-popup modal-danger modal-message" style="position: fixed;  left:45%; width:300px; top: 30%; display:none">
    <div class="modal-content ">
        <div class="modal-header text-center">
            <i class="glyphicon glyphicon-fire"></i>
        </div>
        <div class="modal-title">No Store Selected</div>
        <div class="modal-body ">Please Select Store.</div>
        <div class="modal-footer " style="text-align:center">
            <a class="btn btn-danger" data-dismiss="modal" onclick="closemodal();">Ok </a>
        </div>
    </div>
</div>
<div id="popupSelectAlert" class="divIDClass1 modal-popup modal-danger modal-message" style="position: fixed; left: 40%; width: 300px; top: 30%; background: rgb(251 246 246 / 50%) !important; display: none">
    <div class="modal-content ">
        <div class="modal-header text-center">
            <i class="glyphicon glyphicon-fire"></i>
        </div>
        <div class="modal-title">Please Confirm</div>
        <div class="modal-body " id="bodymsg">Are you sure to select all?.</div>
        <div class="modal-footer " style="text-align:center">
            <a class="btn btn-danger" data-dismiss="modal" onclick="confirmAll();">Ok </a>
            <a class="btn btn-danger" data-dismiss="modal" onclick="ClosemodelNew();">Cancel </a>

        </div>
    </div>
</div>

<script type="text/javascript">
    document.title = '@ViewBag.Title';

    function closemodal() {
        $(".divIDClass").hide();
    }
</script>
<script src="~/Form_JS/CheckListExpense.js"></script>