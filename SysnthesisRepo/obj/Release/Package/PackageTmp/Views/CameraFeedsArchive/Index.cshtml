    <script>
        function closecashmodal() {
            $("#UrlVideo").removeAttr('src');
            $("#divCash").hide();
        }
        function ChangeDate(args) {
            var dateStr = args.value.toUTCString(); // Convert to UTC string
            $.ajax({
                type: "POST",
                url: '@Url.Action("ChangeData")',
                contentType: "application/json", // define content type
                dataType: "json",
                data: JSON.stringify({ 'date': dateStr }),   // pass data to controller
                success: function (response) {
                    var grid = document.getElementById('InlineEditing').ej2_instances[0];
                    grid.refresh();
                }
            });
        }
   
            
    $(document).ready(function () {
        

        var ajax = new ej.base.Ajax({
            url: "@Url.Action("GetList", "CameraFeedsArchive")", //render the partial view
            type: "POST",
            contentType: "application/json"
        });
        ajax.send().then((data) => {
            $("#GridValue").html(data);
        });

    });
    </script>
