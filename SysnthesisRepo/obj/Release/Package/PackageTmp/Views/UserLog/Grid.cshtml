<div id="loadbox"></div>
<div class="row" style="padding: 25px 25px;">
    <div class="col-md-12">
        <h1 class="header">Detail User Log</h1>
        <hr />
    </div>
</div>

<div class="page-header-main position-relative gridPageHeader" style="height: 67px !important; background-color: white;margin: 0px 20px 0px 20px">
    <div class="col-md-3" style="margin-left: 17px;">
        @Html.EJS().DropDownList("User").DataSource((IEnumerable<Object>)ViewBag.User).Fields(new DropDownListFieldSettings { Text = "UserName", Value = "UserId" }).Placeholder("Select User").AllowFiltering(true).Filtering("onfilteringvendor").Render()
    </div>
    <div class="col-md-3" style="margin-left: 17px;">
        <input name="txtfromdate" type="text" maxlength="100" id="txtfromdate" placeholder="From" class="datepicker form-control" onkeydown="return false">
    </div>
    <div class="col-md-2" style="margin-left: 17px;">
        @Html.EJS().DropDownList("Module").DataSource((IEnumerable<Object>)ViewBag.Module).Fields(new DropDownListFieldSettings { Text = "ModuleName", Value = "ModuleName" }).Placeholder("Select Module").Value("All Module").AllowFiltering(true).Filtering("onfilteringModule").Render()

    </div>
    <div class="col-md-2" style="margin-left: 17px;">
        @Html.EJS().DropDownList("ActionValue").DataSource((IEnumerable<Object>)ViewBag.Action).Fields(new DropDownListFieldSettings { Text = "ActionName", Value = "ActionName" }).Placeholder("Select Module").Value("All Action").AllowFiltering(true).Filtering("onfilteringAction").Render()

    </div>
    <div style="display:flex">
        <button type="button" class="btnadddocument" onclick="Filter();" style="width:100px; border:none;">Go</button>
        <button type="button" class="pdfUnclearExportBtn" style="background-color:red" id="pdf" onclick="toolbarClick(this)"><img src="~/Content/Admin/images/icon_pdf.png" alt=""></button>
        &nbsp;&nbsp;
        <button type="button" style="background-color:green" class="exlUnclearExportBtn" id="excel" onclick="toolbarClick(this)"><img src="~/Content/Admin/images/icon_excel.png" alt=""></button>
    </div>

</div>
<div id="gridvaluechange" style="margin: 20px 20px 20px 20px"></div>
<script type="text/javascript">
    document.title = '';
    var Startdate = '@ViewBag.Sdate';

</script>
