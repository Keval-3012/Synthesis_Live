<script>
      function RemoveStore1 (iii)
    {
        
         $.ajax({
                 url: '@Url.Action("RemoveMergeVendor1", "VendorMasters")',
                 type: "POST",
             data: JSON.stringify({ vendorid: iii}),
                 contentType: "application/json; charset=utf-8",
                 dataType: "json",
                 success: function (response) {
                     
                     if (response == "success") {
                         
                         var grid = document.getElementById("grdStore1").ej2_instances[0];
                         grid.dataSource = new ej.data.DataManager({
                             url: "/VendorMasters/AddVendorlistStore1",
                             adaptor: new ej.data.UrlAdaptor()
                         });
                         toastr.success('Remove Successfully.');
                     }
                     else {
                         toastObj.content = "Something went wrong!";
                         toastObj.target = document.body;
                         toastObj.show({ title: 'Error!', cssClass: 'e-toast-danger', icon: 'e-error toast-icons' });
                     }

                 },
                 error: function (response) {
                 }
             });
    }
</script>
<script>


    function add1() {
        
        var data = document.getElementById("VendorList1").ej2_instances[0];
        var item = data.itemData;
        //var store1 = document.getElementById("store1");
        //
        //var row = store1.insertRow(store1.rows.length);
        //var cell1 = row.insertCell(0);
        //var cell2 = row.insertCell(1);
        //cell1.innerHTML = item.VendorId;
        //cell2.innerHTML = item.VendorName;
        if (item === undefined) {
            alert("Please select Vendor..!");
        }
        else {
            var ven = item.VendorName;
            ven = ven.replace('&', '^');
            var grid = document.getElementById("grdStore1").ej2_instances[0];
            grid.dataSource = new ej.data.DataManager({
                url: "/VendorMasters/AddVendorlistStore1?vendorid=" + item.VendorId + "&vendorname=" + ven,
                adaptor: new ej.data.UrlAdaptor()
            });
        }

    }

    function mergedata1() {
        var data = document.getElementById("VendorListmaster1").ej2_instances[0];
        var item = data.itemData;


        
        if (item === undefined) {
            alert("Please select Master Vendor first..!!!");
        }
        else {
            
                $.ajax({
                    url: '@Url.Action("UpdateMergeVendorStore1", "VendorMasters")',
                    type: "POST",
                    data: JSON.stringify({ mastervendorid: item.VendorId }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    beforeSend: function () {
                        $('#loading').css('display', 'block');
                    },
                    success: function (response) {
                        
                        if (response.message == "success") {
                            
                            var grid = document.getElementById("grdStore1").ej2_instances[0];
                            grid.dataSource = new ej.data.DataManager({
                                url: "/VendorMasters/AddVendorlistStore1",
                                adaptor: new ej.data.UrlAdaptor()
                            });
                            
                            var drp = document.getElementById("VendorListmaster1").ej2_instances[0];
                            drp.dataSource = response.obj;
                            drp.dataBind();
                            drp.clear();

                            var drp1 = document.getElementById("VendorList1").ej2_instances[0];
                            drp1.dataSource = response.obj;
                            drp1.dataBind();
                            drp1.clear();
                            toastr.success('Vendor Merge Successfully.');
                        }
                        else {
                            toastObj.content = "Something went wrong!";
                            toastObj.target = document.body;
                            toastObj.show({ title: 'Error!', cssClass: 'e-toast-danger', icon: 'e-error toast-icons' });
                        }
                        $('#loading').css('display', 'none');
                    },
                    error: function (response) {
                    }
                }).done(function () {
                    $('#loading').css('display', 'none');
                });

        }
    }

    function add2() {
        
        var data = document.getElementById("VendorList2").ej2_instances[0];
        var item = data.itemData;
        if (item === undefined) {
            alert("Please select Vendor..!");
        }
        else {
            var ven = item.VendorName;
            ven = ven.replace('&', '^');
            var grid = document.getElementById("grdStore2").ej2_instances[0];
            grid.dataSource = new ej.data.DataManager({
                url: "/VendorMasters/AddVendorlistStore2?vendorid=" + item.VendorId + "&vendorname=" + ven,
                adaptor: new ej.data.UrlAdaptor()
            });
        }
